          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.util.LinkedList

// Create or get a static field from the script context
def getMemoryLeak() {
    if (getBinding().hasVariable(&apos;memoryLeak&apos;)) {
        return getBinding().getVariable(&apos;memoryLeak&apos;)
    } else {
        def memoryLeak = new LinkedList&lt;byte[]&gt;()
        getBinding().setVariable(&apos;memoryLeak&apos;, memoryLeak)
        return memoryLeak
    }
}

// Access the static list
LinkedList&lt;byte[]&gt; memoryLeak = getMemoryLeak()

// Allocate memory
byte[] leak = new byte[100 * 1024 * 1024] // 100 MB
memoryLeak.add(leak)

// Remove the oldest entry if the list size exceeds a threshold to force memory usage
if (memoryLeak.size() &gt; 100) {
    memoryLeak.removeFirst()
}

// Log a message for monitoring (optional)
log.info(&quot;Allocated 1MB of memory, current list size: ${memoryLeak.size()}&quot;)

// Simulate processing time
Thread.sleep(100)</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223Sampler>
